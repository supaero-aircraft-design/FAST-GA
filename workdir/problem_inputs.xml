<FASTOAD_model>
  <data>
    <handling_qualities>
      <stick_fixed_static_margin is_input="True">nan<!--stick fixed static margin--></stick_fixed_static_margin>
      <static_margin>
        <target is_input="True">0.25<!--static margin we want to achieve--></target>
      </static_margin>
    </handling_qualities>
    <geometry>
      <wing>
        <MAC>
          <length units="m" is_input="True">nan<!--length of mean aerodynamic chord of wing--></length>
        </MAC>
      </wing>
    </geometry>
    <weight>
      <aircraft>
        <CG>
          <aft>
            <MAC_position is_input="True">nan<!--most aft X-position of center of gravity as ratio of mean aerodynamic chord--></MAC_position>
            <x units="m" is_input="True">nan<!--most aft X-position of aircraft center of gravity--></x>
          </aft>
        </CG>
      </aircraft>
    </weight>
  </data>
</FASTOAD_model>
