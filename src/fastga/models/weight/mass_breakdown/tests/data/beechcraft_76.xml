<FASTOAD_model>
	<data>
		<TLAR>
			<NPAX_design>2.0<!--top-level requirement: number of passengers in cabin for design mission--></NPAX_design>
			<luggage_mass_design units="kg">30.0<!--luggage mass to carry during design mission--></luggage_mass_design>
			<v_cruise units="kn">158.0<!--top-level requirement: cruise speed--></v_cruise>
			<v_limit units="kn">194.0<!--top-level requirement: limit speed--></v_limit>
			<v_max_sl units="kn">171.0<!--top-level requirement: max level velocity--></v_max_sl>
		</TLAR>
		<geometry>
			<cabin>
				<luggage>
					<mass_max units="kg">50.0<!--maximum luggage mass to carry--></mass_max>
				</luggage>
				<seats>
					<passenger>
						<NPAX_max>2.0<!--maximum number of passengers in cabin (2 pilots seats not included)--></NPAX_max>
					</passenger>
				</seats>
			</cabin>
			<fuselage>
				<length units="m">8.888<!--total fuselage length--></length>
				<front_length units="m">2.274<!--length of front non-cylindrical part of the fuselage--></front_length>
				<rear_length units="m">2.924<!--length of front non-cylindrical part of the fuselage--></rear_length>
				<maximum_height units="m">1.338<!--maximum fuselage height--></maximum_height>
				<maximum_width units="m">1.198<!--maximum fuselage width--></maximum_width>
				<wet_area units="m**2">33.354<!--wet area of fuselage--></wet_area>
